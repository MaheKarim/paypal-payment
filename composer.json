{
    "name": "mahekarim/paypal-payment",
    "description": "A package to handle PayPal payments.",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "guzzlehttp/guzzle": "^7.0" 
    },
    "autoload": {
        "psr-4": {
            "MaheKarim\\PaypalPayment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MaheKarim\\PaypalPayment\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^11.4",
        "mockery/mockery": "^1.6"
    }
}
